{
  "manifest_version": 2,
  "name": "Playback Rate",
  "description": "Control the playback rate of HTML5 <video> and <audio> elements",
  "version": "1.2",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "contextMenus",
    "tabs",
    "<all_urls>"
  ],
  "content_scripts": [
    {
      "matches": ["*://vimeo.com/*"],
      "js": ["vimeo-user-script.js"],
      "css": ["vimeo-styles.css"]
    }
  ]
}
